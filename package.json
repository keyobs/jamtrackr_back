{
  "name": "jamtrackr_back",
  "author": "keyobs",
  "version": "1.0.0",
  "description": "backend-core",
  "main": "index.ts",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/keyobs/jamtrackr_back.git"
  },
  "scripts": {
    "test": "error \"no test specified\" && exit 1",
    "start": "node index.ts",
    "server": "nodemon src/server.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint --ignore-path .eslintignore \"src/**/*.ts\" --fix",
    "prepare": "husky install",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "eslint --fix"
    ],
    "src/**/*.{js,ts,json}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "precommit"
    }
  },
  "bugs": {
    "url": "https://github.com/keyobs/jamtrackr_back/issues"
  },
  "homepage": "https://github.com/keyobs/jamtrackr_back#readme",
  "dependencies": {
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "nodemon": "^3.0.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
